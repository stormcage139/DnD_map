{
  "editor.mouseWheelZoom": true,
  "files.associations": {
        "/*.html": "html",
        "/templates/*/*.html": "django-html",
        "/templates/*/*/*.html": "django-html",
        "/templates/*": "django-html",
        "/requirements{/,*}.{txt,in}": "pip-requirements"
    },
    "emmet.includeLanguages": {
      "django-html": "html"
    },
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
